entrypoint = "main.py"
modules = ["cpp-clang14"]

[nix]
channel = "stable-24_05"
packages = ["qemu_full"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[workflows]
runButton = "Build SCos"

[[workflows.workflow]]
name = "Build SCos"
author = 37620234
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd SCos && make clean && make"
